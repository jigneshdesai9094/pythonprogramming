   "source": [
    "term=int(input(\"Enter No of Term : \"))\n",
    "sum=0\n",
    "i=1\n",
    "no=1\n",
    "\n",
    "while(i<=term):\n",
    "    sum=0\n",
    "    for j in range(1,int(no/2)+1):\n",
    "        if(no%j==0):\n",
    "            sum=sum+j\n",
    "            \n",
    "    if(no==sum):\n",
    "        print(no)\n",
    "        i+=1\n",
    "    no+=1\n",
    "    \n",
